        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Debug train",
            "type": "debugpy",
            "request": "launch",
            "program": "train.py",
            "console": "integratedTerminal",
            "args": ["--group=debug","--name=debug","--yaml=colmap","--data.scene=oats","--model=barf","--visdom=False","--data.image_size=[1080,1920]"],
        }
    ]
